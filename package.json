{
    "license": "MIT",
    "name": "cloudreve-cli",
    "version": "0.0.2",
    "main": "src/default.ts",
    "oclif": {
        "commands": "./src/commands",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "files": [
        "/src"
