        "lint": "eslint --ext .js,.ts src",
        "build": "ttsc"
    },
    "devDependencies": {
        "@types/config": "^0.0.38",
        "@types/fs-extra": "^9.0.11",
        "@types/nconf": "^0.10.0",
        "@types/node": "^15.12.4",
        "@types/node-fetch": "^2.5.10",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "cross-env": "^7.0.3",
        "eslint": "^7.29.0",
        "eslint-config-indexyz": "^1.0.2",
        "pkg": "^5.2.1",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "^3.9.0",
        "ttypescript": "^1.5.12",
        "typescript": "^4.3.4"
    },
    "dependencies": {
        "@oclif/command": "^1.8.0",
        "@oclif/config": "^1.17.0",
        "@oclif/plugin-help": "^3.2.2",
        "config": "^3.3.6",
        "fs-extra": "^10.0.0",
        "nconf": "^0.11.3",
        "node-fetch": "^2.6.1",
        "ora": "^5.4.1",
        "reflect-metadata": "^0.1.13",
        "tty-table": "^4.1.3"
    },
    "bin": "./src/default.js",
    "pkg": {
        "scripts": [
            "./src/**/*.js"
        ],
        "assets": [
            "./package.json"
        ]
